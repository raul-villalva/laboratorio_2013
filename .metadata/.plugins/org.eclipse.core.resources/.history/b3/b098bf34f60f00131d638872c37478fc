package primero;

import java.util.ArrayList;

public class Paint {
	
	private ArrayList<FiguraGeometrica> paleta = new ArrayList<FiguraGeometrica>();
	
	
	public void init(){
		Circulo fg= new Circulo();
		fg.setColor("azul");
		fg.setRadio(2);
		 fg= new Circulo();
		fg.setColor("amarillo");
		fg.setRadio(3);
		paleta.add(fg);
		
	}


	public ArrayList<FiguraGeometrica> getPaleta() {
		return paleta;
	}


	public void setPaleta(ArrayList<FiguraGeometrica> paleta) {
		this.paleta = paleta;
	}

}
