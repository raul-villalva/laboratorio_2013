public class Suma { 
 public static void main(String[] args){ 
 int suma=0; 
try {
		 for(int i=0;i<=args.length;i++){ 
			 try{
			 suma+= Integer.parseInt(args[i]);
			 }
		 
		 catch(NumberFormatException nfe){
		   System.out.print("La suma es:"+suma); 
		   }
		 } 
		 
 } catch (NullPointerException e){
	 
 }
	 
 }
 

 
